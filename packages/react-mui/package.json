{
  "name": "@forrestjs/react-mui",
  "version": "0.7.1-alpha.0",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "rimraf node_modules && rimraf dist",
    "build": "npm run types && rimraf dist && cross-env NODE_ENV=production babel src --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
    "types": "rimraf types && npx -p typescript tsc src/index.js --declaration --allowJs --emitDeclarationOnly --outDir types --skipLibCheck"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "@mui/styles": "^5.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "typescript": "^4.8.4"
  }
}
